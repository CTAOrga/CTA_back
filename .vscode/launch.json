{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI — Uvicorn (DEBUG)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app.main:app", "--host", "127.0.0.1", "--port", "8000"],
      "jinja": false,
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "FastAPI — Uvicorn (RELOAD)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app.main:app", "--reload", "--port", "8000"],
      "jinja": false,
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
